<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.asiacomBigData.boot.mapper.UserMapper">
    <select id="getUserById" resultType="com.asiacomBigData.boot.model.User" parameterType="java.lang.Integer">
        select name, age,password from user WHERE id=#{id,jdbcType=INTEGER};
    </select>

</mapper>